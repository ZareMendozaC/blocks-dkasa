{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "dkasa/slider-archive-proyectos",
  "version": "1",
  "title": "Slider Listado",
  "category": "dkasa",
  "icon": "building",
  "description": "Slider Listado de Proyectos",
  "attributes": {
    "titulo": {
      "type": "string",
      "default": ""
    },
    "backgroundColor": {
      "type": "string",
      "default": "#BE1521"
    },
    "slides": {
      "type": "array",
      "default": [
        {
          "mediaId": null,
          "mediaUrl": "",
          "nombre": "",
          "area": "",
          "dormitorios": "",
          "ubicacion": "",
          "precio": "",
          "textoEnlace": "",
          "enlace": "",
          "estado": "",
          "botones": {
            "type": "array",
            "default": [{
              "nombre": ""
            }
            ]
          }
        }
      ]
    }
  }
}
